# 前端环境变量
VITE_API_URL=http://localhost:5000/api
GEMINI_API_KEY=your_gemini_api_key

# 服务端环境变量（在 server/.env 中配置）
# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_NAME=blueprint_saas
